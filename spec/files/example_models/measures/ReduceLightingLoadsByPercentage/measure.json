{
  "classname": "ReduceLightingLoadsByPercentage",
  "path": "./measures/ReduceLightingLoadsByPercentage",
  "name": "Reduce Lighting Loads by Percentage",
  "measure_type": "RubyMeasure",
  "arguments": [
    {
      "local_variable": "space_type",
      "variable_type": "Choice",
      "name": "space_type",
      "display_name": "Apply the Measure to a Specific Space Type or to the Entire Model.",
      "default_value": "*Entire Building*",
      "choices": [
        "*Entire Building*"
      ]
    },
    {
      "local_variable": "lighting_power_reduction_percent",
      "variable_type": "Double",
      "name": "lighting_power_reduction_percent",
      "display_name": "Lighting Power Reduction (%).",
      "default_value": 30.0
    },
    {
      "local_variable": "material_and_installation_cost",
      "variable_type": "Double",
      "name": "material_and_installation_cost",
      "display_name": "Increase in Material and Installation Cost for Lighting per Floor Area (%).",
      "default_value": 0.0
    },
    {
      "local_variable": "demolition_cost",
      "variable_type": "Double",
      "name": "demolition_cost",
      "display_name": "Increase in Demolition Costs for Lighting per Floor Area (%).",
      "default_value": 0.0
    },
    {
      "local_variable": "years_until_costs_start",
      "variable_type": "Integer",
      "name": "years_until_costs_start",
      "display_name": "Years Until Costs Start (whole years).",
      "default_value": 0
    },
    {
      "local_variable": "demo_cost_initial_const",
      "variable_type": "Bool",
      "name": "demo_cost_initial_const",
      "display_name": "Demolition Costs Occur During Initial Construction?",
      "default_value": false
    },
    {
      "local_variable": "expected_life",
      "variable_type": "Integer",
      "name": "expected_life",
      "display_name": "Expected Life (whole years).",
      "default_value": 15
    },
    {
      "local_variable": "om_cost",
      "variable_type": "Double",
      "name": "om_cost",
      "display_name": "Increase O & M Costs for Lighting per Floor Area (%).",
      "default_value": 0.0
    },
    {
      "local_variable": "om_frequency",
      "variable_type": "Integer",
      "name": "om_frequency",
      "display_name": "O & M Frequency (whole years).",
      "default_value": 1
    }
  ]
}