{
  "classname": "XcelEDATariffSelectionandModelSetup",
  "path": "./measures/XcelEDATariffSelectionandModelSetup",
  "name": "Xcel EDA Tariff Selection and Model Setup",
  "measure_type": "EnergyPlusMeasure",
  "arguments": [
    {
      "local_variable": "elec_tar",
      "variable_type": "Choice",
      "name": "elec_tar",
      "display_name": "Select an Electricity Tariff.",
      "default_value": "Secondary General",
      "choices": [
        "Residential General",
        "Commercial",
        "Primary General",
        "Secondary General Low Load Factor",
        "Secondary General",
        "Secondary Photovoltaic Time-of-Use",
        "Transmission General",
        "Non-Xcel Commercial",
        "Non-Xcel Primary General",
        "Non-Xcel Secondary General Low Load Factor",
        "Non-Xcel Secondary General",
        "Non-Xcel Transmission General"
      ]
    },
    {
      "local_variable": "gas_tar",
      "variable_type": "Choice",
      "name": "gas_tar",
      "display_name": "Select a Gas Tariff.",
      "default_value": "Large CG",
      "choices": [
        "Residential Gas",
        "Small CG",
        "Large CG",
        "Interruptible Industrial G",
        "Non-Xcel Gas Firm",
        "Non-Xcel Gas Interruptible"
      ]
    }
  ]
}