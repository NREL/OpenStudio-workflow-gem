{
  "classname": "SetWindowToWallRatioByFacade",
  "path": "./measures/SetWindowToWallRatioByFacade",
  "name": "Set Window to Wall Ratio by Facade",
  "measure_type": "RubyMeasure",
  "arguments": [
    {
      "local_variable": "wwr",
      "variable_type": "Double",
      "name": "wwr",
      "display_name": "Window to Wall Ratio (fraction).",
      "default_value": 0.4
