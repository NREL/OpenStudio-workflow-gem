    {
      "local_variable": "sillHeight",
      "variable_type": "Double",
      "name": "sillHeight",
      "display_name": "Sill Height (in).",
      "default_value": 30.0
    },
    {
      "local_variable": "facade",
      "variable_type": "Choice",
      "name": "facade",
      "display_name": "Cardinal Direction.",
